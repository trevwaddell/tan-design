{
  "name": "react-sketch-starter",
  "version": "1.0.0",
  "description": "starter for React Sketch App",
  "main": "index.js",
  "skpm": {
    "main": "react-sketch-starter.sketchplugin",
    "manifest": "source/manifest.json"
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "render": "skpm-build --watch --run",
    "render:once": "skpm-build --run",
    "postinstall": "yarn build && skpm-link"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-sketchapp": "^2.0.0",
    "react-test-renderer": "^16.4.1"
  },
  "devDependencies": {
    "@skpm/builder": "^0.5.7"
  }
}
